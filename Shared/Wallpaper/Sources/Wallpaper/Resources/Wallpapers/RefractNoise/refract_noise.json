    {
      "id": "noiseStrength",
      "type": "float",
      "label": "Noise Strength",
      "default": 0.15,
      "range": { "min": 0.05, "max": 0.3 }
    }
  ],
  "shaderArguments": [],
  "accent": {
    "hue": 23,
    "saturation": 0.5,
    "brightness": 1.0
  },
  "material": {
    "foreground": "light",
    "blurRadius": 8.0,
    "overlay": {
      "opacity": 0.0,
      "darkness": 0.0
