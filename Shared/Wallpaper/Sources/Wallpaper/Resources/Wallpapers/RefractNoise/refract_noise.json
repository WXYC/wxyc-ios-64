    {
      "id": "tileColorG",
      "type": "float",
      "label": "Tile Green",
      "default": 0.76,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "tileColorB",
      "type": "float",
      "label": "Tile Blue",
      "default": 0.78,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "refractIndex",
      "type": "float",
      "label": "Refract Index",
