{
  "id": "lava_lite",
  "displayName": "Lava Lite",
  "version": "1.0.0",
  "renderer": {
    "type": "rawMetal",
    "vertexFunction": "fullscreenVertex",
    "fragmentFunction": "lavaLiteFragment",
    "timeScale": 0.5
  },
  "parameters": [
    {
      "id": "blobColor1R",
      "type": "float",
      "label": "Blob 1 Red",
      "default": 0.1,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "blobColor1G",
      "type": "float",
      "label": "Blob 1 Green",
      "default": 0.9,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "blobColor1B",
      "type": "float",
      "label": "Blob 1 Blue",
      "default": 0.8,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "blobColor2R",
      "type": "float",
      "label": "Blob 2 Red",
      "default": 0.9,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "blobColor2G",
      "type": "float",
      "label": "Blob 2 Green",
      "default": 0.1,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "blobColor2B",
      "type": "float",
      "label": "Blob 2 Blue",
      "default": 0.6,
      "range": { "min": 0.0, "max": 1.0 }
    },
    {
      "id": "noiseScale",
      "type": "float",
      "label": "Noise Scale",
      "default": 4.0,
      "range": { "min": 2.0, "max": 8.0 }
    },
    {
      "id": "blobThreshold",
      "type": "float",
      "label": "Blob Threshold",
      "default": 0.6,
      "range": { "min": 0.4, "max": 0.8 }
    }
  ],
  "shaderArguments": [],
  "foreground": "dark",
  "accent": {
    "hue": 23,
    "saturation": 0.75,
    "brightness": 1.0
  },
  "blurRadius": 12.0,
  "overlayOpacity": 0.1,
  "overlayIsDark": true
}
