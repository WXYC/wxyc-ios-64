{
  "id": "lava_lite",
  "displayName": "Lava Lite",
  "version": "1.0.0",
  "renderer": {
    "type": "rawMetal",
    "vertexFunction": "fullscreenVertex",
    "fragmentFunction": "lavaLiteFragment",
    "timeScale": 0.5
  },
  "parameters": [
    {
      "id": "brightness",
      "type": "float",
      "label": "Brightness",
      "default": 1.0,
      "range": { "min": 0.0, "max": 2.0 }
    }
  ],
  "shaderArguments": [],
  "accent": {
    "hue": 23,
    "saturation": 0.75,
    "brightness": 1.0
  },
  "material": {
    "foreground": "dark",
    "blurRadius": 12.0,
    "overlay": {
      "opacity": 0.1,
      "darkness": 1.0
    }
  }
}
